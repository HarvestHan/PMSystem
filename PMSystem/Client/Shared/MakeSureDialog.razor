<MudDialog>
    <DialogContent>
        确定执行此操作吗?
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Warning" OnClick="Cancel">取消</MudButton>
        <MudButton Color="Color.Success" OnClick="Submit">确定</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}